    "import numpy as np\n",
    "\n",
    "def preprocess_image(image_path, target_size=(256, 256)):\n",
    "    image = cv2.imread(image_path)\n",
    "    image = cv2.resize(image, target_size)\n",
    "    image = image.astype('float32') / 255.0  # Normalizaci√≥n\n",
    "    return image"
